{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
        "require": false,
        "$": false,
        "jQuery": false,
        "L": false,
        "ApiFetcher": false,
        "FormStorage": false,
